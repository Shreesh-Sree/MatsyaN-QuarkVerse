
# FisherMate.AI Environment Variables
# Copy this file to .env.local and fill in your actual API keys
# NEVER commit .env.local or files with real API keys to version control

# Google Genkit AI API Key (required for AI features)
# Get this from: https://aistudio.google.com/app/apikey
GOOGLE_GENAI_API_KEY=your_google_genai_api_key_here

# Alternative Google Generative AI API Key (if using different service)
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key_here

# Google Maps API Key (required for map functionality)
# Get this from: https://console.cloud.google.com/google/maps-apis
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Firebase Configuration (required for authentication and data)
# Get these from: https://console.firebase.google.com/project/your-project/settings/general
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id_here
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id_here

# Google Translate API Key (Optional)
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable the Cloud Translation API in your Google Cloud Console
GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key_here

# Development Environment
NODE_ENV=development

# Optional: Additional API Keys for external services
# WEATHER_API_KEY=your_weather_api_key_here
# OCEAN_DATA_API_KEY=your_ocean_data_api_key_here

NEXT_PUBLIC_APP_URL=http://localhost:3000
